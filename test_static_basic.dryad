// test_static_basic.dryad
// Very basic test for static method functionality

class TestClass {
    public static fun hello() {
        return "Hello from static method";
    }
    
    public static fun getValue() {
        return 42;
    }
}

// Test static method calls
let message = TestClass.hello();
print(message);

let number = TestClass.getValue();
print(number);

print("Static method basic test completed!");
