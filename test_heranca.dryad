// Teste incremental - só herança simples
class Animal {
    protected nome;
    
    function init(nome) {
        this.nome = nome;
    }
    
    public function getNome() {
        return this.nome;
    }
}

class Cachorro extends Animal {
    private raca;
    
    function init(nome, raca) {
        this.nome = nome;
        this.raca = raca;
    }
    
    public function getRaca() {
        return this.raca;
    }
}

// Teste herança simples (sem super)
let rex = Cachorro("Rex", "Labrador");
let nome = rex.getNome();
let raca = rex.getRaca();
