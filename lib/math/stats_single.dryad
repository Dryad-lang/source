// lib/math/stats.dryad
// Funções estatísticas

namespace Math {
    export class Stats {
        // Média aritmética
        public static fn mean(list) {
            let total = 0;
            let i = 0;
            while (i < list.length) {
                total = total + list[i];
                i = i + 1;
            }
            return total / list.length;
        }
    }
}
