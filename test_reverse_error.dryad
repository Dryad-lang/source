// test_reverse_error.dryad
// Test calling instance method as static

class TestClass {
    public static fun staticMethod() {
        return "static";
    }
    
    public fun instanceMethod() {
        return "instance";
    }
}

// Valid calls work
print("Valid calls:");
let result1 = TestClass.staticMethod();
print(result1);

let obj = new TestClass();
let result2 = obj.instanceMethod();
print(result2);

// Now test error: calling instance method as static
print("Trying to call instance method as static (should error):");
let errorResult = TestClass.instanceMethod();
