// EXPECTED: Hello from Console!\nType of 42: number
// Teste: Simulação de multiple using statements

class Console {
    public static fun print(message) {
        print(message);
    }
}

class Types {
    public static fun typeof(value) {
        return "number";
    }
}

Console.print("Hello from Console!");
let type_name = Types.typeof(42);
print("Type of 42: " + type_name);
