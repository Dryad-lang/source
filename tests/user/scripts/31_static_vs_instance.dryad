// tests/user/scripts/31_static_vs_instance.dryad
// EXPECTED: 10\n30
// Teste: Comparação entre métodos estáticos e de instância

class Calculator {
    // Método estático
    static fun multiply(a, b) {
        return a * b;
    }
    
    // Método de instância
    fun add(a, b) {
        return a + b;
    }
}

// Uso de método estático (sem instanciar)
let static_result = Calculator.multiply(2, 5);
print(static_result);  // 10

// Uso de método de instância (requer instância)
let calc = Calculator();
let instance_result = calc.add(10, 20);
print(instance_result);  // 30
