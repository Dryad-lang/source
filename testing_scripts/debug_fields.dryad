// Debug with field assignment
namespace Math.Utils {
    class Vector {
        public x;
        public y;
        
        fun init(x, y) {
            print("Setting this.x");
            this.x = x;
            print("Setting this.y");
            this.y = y;
            print("Init finished");
        }
        
        fun name() {
            return "Vector2D";
        }
    }
}

print("Creating vector...");
let vec = Math.Utils.Vector(3, 4);
print("Done");
