// Teste básico do sistema de namespaces (sem imports por enquanto)

// Definir alguns módulos com namespaces para testar
namespace Core {
    export class Types {
        public static fun typeof(value) {
            return "mock_type";
        }
        
        public static fun isString(value) {
            return true;
        }
    }
}

namespace IO {
    export class Console {
        public static fun println(message) {
            print(message);
            return true;
        }
    }
}

// Testar uso direto dos namespaces
print("Testing namespace access:");

// Tentar acessar diretamente
let result1 = Core.Types.typeof("test");
print("Core.Types.typeof result:");
print(result1);

let result2 = IO.Console.println("Hello from namespace");
print("IO.Console.println completed");

print("Namespace tests completed!");
