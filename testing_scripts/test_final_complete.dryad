// Teste completo do sistema using/export

print("=== TESTE COMPLETO DO SISTEMA USING/EXPORT ===");

// 1. Export global
export fun globalFunction() {
    return "Global function works!";
}

// 2. Namespace com exports
namespace MyLibrary {
    export fun libraryFunction() {
        return "Library function works!";
    }
    
    export class LibraryClass {
        fun test() {
            return "Library class works!";
        }
    }
}

// 3. Using com alias
use MyLibrary as Lib;

// 4. Testes
print("1. Export global:");
let result1 = globalFunction();
print(result1);

print("2. Namespace usando alias:");
let result2 = Lib.libraryFunction();
print(result2);

let obj = Lib.LibraryClass();
let result3 = obj.test();
print(result3);

// 5. Importação de módulo externo
print("3. Módulo externo:");
use simple_math as Math;
let mathResult = Math.add(10, 15);
print("10 + 15 =");
print(mathResult);

let calculator = Math.Calculator();
let calcResult = calculator.add(20, 30);
print("Calculator: 20 + 30 =");
print(calcResult);

print("=== TODOS OS TESTES PASSARAM! ===");
