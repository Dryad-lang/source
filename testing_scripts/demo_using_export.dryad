// RESUMO DO SISTEMA USING/EXPORT IMPLEMENTADO
// ============================================

// 1. EXPORT básico de funções e classes
export fun utilityFunction() {
    return "Exported utility function";
}

export class ExportedClass {
    fun method() {
        return "Method from exported class";
    }
}

// 2. NAMESPACES com export
namespace Library.Core {
    export fun coreFunction() {
        return "Core library function";
    }
    
    export class CoreClass {
        fun process() {
            return "Core processing";
        }
    }
}

// 3. USING com alias
use Library.Core as Core;

// 4. USING sem alias (usa último nome do path)
use Library.Core;

// 5. Uso das funcionalidades importadas
print("=== Sistema Using/Export - Demonstração ===");

// Usando export global
let result1 = utilityFunction();
print(result1);

let obj1 = ExportedClass();
let result2 = obj1.method();
print(result2);

// Usando namespace com alias
let result3 = Core.coreFunction();
print(result3);

let obj2 = Core.CoreClass();
let result4 = obj2.process();
print(result4);

// Usando namespace sem alias
let result5 = Core.coreFunction();
print(result5);

print("=== Importação de módulo externo ===");
use simple_math as Math;
let externalResult = Math.add(50, 75);
print("50 + 75 =");
print(externalResult);

print("=== Sistema using/export funcionando! ===");
