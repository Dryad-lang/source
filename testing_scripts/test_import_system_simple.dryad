// Teste simples do sistema de imports

print("=== Simple Import System Test ===");

// Definir módulos localmente (simulando imports)
class Types {
    public static fun typeof(value) {
        return "string";
    }
    
    public static fun isString(value) {
        return true;
    }
}

class Console {
    public static fun println(message) {
        print(message);
        return true;
    }
}

class FileSystem {
    public static fun fileExists(filename) {
        return false;
    }
}

// Testar uso dos módulos
print("Testing Types module:");
let valueType = Types.typeof("test");
print(valueType);

let isString = Types.isString("test");
print(isString);

print("Testing Console module:");
let result = Console.println("Hello from Console");
print(result);

print("Testing FileSystem module:");
let exists = FileSystem.fileExists("test.txt");
print(exists);

print("=== All modules working correctly! ===");
