// test_static_method_calls.dryad
// Teste para verificar chamadas de métodos estáticos

class TestClass {
    public static fun getValue() {
        return "test_value";
    }
    
    public static fun getValueAlias() {
        return TestClass.getValue();
    }
    
    public static fun simpleMethod() {
        return "simple";
    }
}

print("Testing direct static method call:");
let result1 = TestClass.getValue();
print(result1);

print("Testing static method calling another static method:");
let result2 = TestClass.getValueAlias();
print(result2);

print("Testing simple static method:");
let result3 = TestClass.simpleMethod();
print(result3);

print("Static method call tests completed!");
