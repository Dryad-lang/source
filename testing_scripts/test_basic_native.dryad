// test_basic_native.dryad
// Teste básico de funções nativas

native_console_println("Testando funções nativas básicas");

let num = 42;
native_console_println("Número:", num);

let texto = "hello";
native_console_println("Texto:", texto);

let tipoNum = native_core_typeof(num);
native_console_println("Tipo do número:", tipoNum);

let tipoTexto = native_core_typeof(texto);
native_console_println("Tipo do texto:", tipoTexto);

let numString = native_core_to_string(num);
native_console_println("Número como string:", numString);

native_console_println("Teste básico finalizado");
