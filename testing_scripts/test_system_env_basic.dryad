// test_system_env_basic.dryad
// Teste básico para o módulo Environment

namespace System {
    export class Environment {
        public static fun get(name) {
            return "mock_value";
        }
        
        public static fun set(name, value) {
            return "success";
        }
        
        public static fun exists(name) {
            return true;
        }
        
        public static fun hasVar(name) {
            return Environment.exists(name);
        }
    }
}

// Testar o módulo Environment
print("Testing System.Environment:");

let result1 = System.Environment.get("PATH");
print(result1);

let result2 = System.Environment.set("TEST_VAR", "test_value");
print(result2);

let result3 = System.Environment.exists("TEST_VAR");
print(result3);

let result4 = System.Environment.hasVar("TEST_VAR");
print(result4);

print("System.Environment test completed!");
