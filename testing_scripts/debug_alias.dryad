// Debug alias resolution
namespace Math {
    class PI {
        fun value() {
            return 3.14159;
        }
    }
}

print("Before using");
use Math as M;
print("After using");

print("Trying direct access...");
let pi1 = Math.PI();
let val1 = pi1.value();
print(val1);

print("Trying alias access...");
let pi2 = M.PI();
let val2 = pi2.value();
print(val2);
