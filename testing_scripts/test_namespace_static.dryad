// test_namespace_static.dryad
// Teste para verificar métodos estáticos em namespaces

// Primeiro, teste sem namespace
class SimpleClass {
    public static fun getValue() {
        return "simple_value";
    }
}

// Agora teste com namespace
namespace TestNS {
    export class NamespaceClass {
        public static fun getValue() {
            return "namespace_value";
        }
    }
}

print("Testing simple static method:");
let result1 = SimpleClass.getValue();
print(result1);

print("Testing namespace static method:");
let result2 = TestNS.NamespaceClass.getValue();
print(result2);

print("Test completed!");
