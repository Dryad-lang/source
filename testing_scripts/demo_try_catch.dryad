// demo_try_catch.dryad
// Demonstração final do sistema try-catch

print("=== DEMONSTRAÇÃO TRY-CATCH ===");

// 1. Try-catch básico
print("1. Try-catch básico funcionando:");
try {
    print("Executando código normal...");
    let x = 10;
    print("Valor de x:");
    print(x);
} catch (e) {
    print("Este catch não deveria executar");
}

// 2. Capturando exceção
print("2. Capturando exceção:");
try {
    print("Antes do throw...");
    throw "Esta é uma exceção de teste!";
    print("Esta linha nunca executa");
} catch (error) {
    print("Exceção capturada com sucesso:");
    print(error);
}

// 3. Throw de número
print("3. Throw de número:");
try {
    throw 123;
} catch (num_error) {
    print("Número capturado:");
    print(num_error);
}

// 4. Try-catch aninhado
print("4. Try-catch aninhado:");
try {
    print("Try externo...");
    try {
        print("Try interno...");
        throw "Erro interno";
    } catch (inner) {
        print("Capturado internamente:");
        print(inner);
    }
    print("Continuando após catch interno...");
} catch (outer) {
    print("Este catch externo não deveria executar");
}

print("=== TRY-CATCH IMPLEMENTADO COM SUCESSO! ===");
