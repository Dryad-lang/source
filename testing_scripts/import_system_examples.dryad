// import_system_examples.dryad
// Diferentes formas de importar e usar módulos

// 1. Import completo de namespace
using IO.*;  // Importa todas as classes do namespace IO

// 2. Import de classe específica
using Core.Types;  // Importa apenas a classe Types

// 3. Import com alias
using System.Environment as Env;  // Importa Environment com alias

// 4. Import múltiplo
using Core.Types, Core.Meta, IO.Console;

// Exemplos de uso:

print("=== Import System Examples ===");

// Usando classe importada diretamente
let value = 42;
let type = Types.typeof(value);
print("Type of 42: " + type);

// Usando Console importado do namespace IO
Console.println("Hello from Console!");

// Usando alias
let homeDir = Env.get("HOME");
print("Home directory retrieved using alias");

// Usando classe do namespace completo importado
let buffer = Buffer.create(100);
print("Buffer created from IO namespace");

print("=== All import examples completed ===");
