// test_logical_operators.dryad
// Teste dos operadores lógicos

native_console_println("=== Teste Operadores Lógicos ===");

// Teste operador NOT (!)
let isTrue = true;
let isFalse = false;

native_console_println("true:", isTrue);
native_console_println("!true:", !isTrue);
native_console_println("false:", isFalse);
native_console_println("!false:", !isFalse);

// Teste operadores AND (&&) e OR (||)
native_console_println("\nOperadores && e ||:");
native_console_println("true && true:", true && true);
native_console_println("true && false:", true && false);
native_console_println("false && true:", false && true);
native_console_println("false && false:", false && false);

native_console_println("true || true:", true || true);
native_console_println("true || false:", true || false);
native_console_println("false || true:", false || true);
native_console_println("false || false:", false || false);

// Teste com números
native_console_println("\nTeste com números:");
let x = 5;
let y = 0;

native_console_println("x:", x, "!x:", !x);
native_console_println("y:", y, "!y:", !y);

// Teste condicionais
native_console_println("\nTeste condicionais:");
if (true && !false) {
    native_console_println("Condição (true && !false) é verdadeira");
}

if (false || true) {
    native_console_println("Condição (false || true) é verdadeira");
}

native_console_println("=== Teste Concluído ===");
